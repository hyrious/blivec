{
  "name": "@hyrious/blivec",
  "version": "0.3.20",
  "description": "bilibili live cli",
  "type": "module",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "bin": {
    "bl": "./dist/bin.js"
  },
  "files": [
    "src",
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "node dist/bin.js",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:js && npm run build:types",
    "build:js": "esbuild src/index.ts src/bin.ts --target=node16.15 --outdir=dist",
    "build:types": "dts src/index.ts -o dist/index.d.ts"
  },
  "engines": {
    "node": ">=16.15"
  },
  "repository": "hyrious/blivec",
  "keywords": [
    "bilibili",
    "danmaku",
    "stream"
  ],
  "author": "hyrious <hyrious@outlook.com>",
  "license": "MIT",
  "devDependencies": {
    "@hyrious/dts": "^0.1.9",
    "@hyrious/rimraf": "^0.1.0",
    "@types/node": "^18.16.19",
    "esbuild": "^0.18.11"
  }
}
